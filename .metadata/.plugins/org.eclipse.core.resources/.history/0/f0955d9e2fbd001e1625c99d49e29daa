package thread;


class Ye
{
	 for(int i=1;i<=5;i++)
	 {
		 System.out.println("hi");
	 }
}

public class YeildEx extends Ye Runnable 
{
	public void run ()
	{
		System.out.println("Say Hi");
	}
	public static void main(String[] args) 
	{
		YeildEx e=new YeildEx();
		Thread df=new Thread(e);
		df.start();
	}

}